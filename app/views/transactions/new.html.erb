<div class="page-header">
  <h1>Transactions</h1>
</div>
<div id="transactions">
  <ul class="tabs" data-tabs="tabs">
    <li><%= link_to "Transaction History", transactions_path %></li>
    <li class="active"><%= link_to "Record Transaction", new_transaction_path %></li>
    <li><a href="#upload_transactions">Upload Transactions</a></li>
  </ul>
  <div class="tab-content">
    <div class="active" id="record_transaction"> 
      <%= render 'form' %>
    </div>
    <div id="upload_transactions">
      <%= render 'upload_form' %>
    </div>
  </div>
</div>

<h1>Accounts</h1>
<%= link_to 'Add Account', new_account_path %>
<div id="accounts">
  <table>
    <tr>
      <th>Name</th>
      <th>Balance</th>
      <th>Note</th>
      <th>Action</th>
    </tr>
  <% @accounts.each do |a| %>
    <tr>
      <td><%= a.name %></td>
      <td></td>
      <td><%= a.note %></td>
      <td></td>
    </tr>
  <% end %>
  </table>
</div>
